<%= form_with(model: like) do |form| %>
  <div class="d-flex align-items-center">
    <%= form.hidden_field :fan_id, value: current_user.id %>
    <%= form.hidden_field :photo_id, value: photoid %>
    <%= form.submit "Like", class: "btn btn-link link-underline-dark link-underline-opacity-0 link-underline-opacity-100-hover" %>
  </div>
<% end %>
